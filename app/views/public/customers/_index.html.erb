<h2>投稿一覧</h2>
<div class="row card-wrapper mr-2">
  <% post_images.each do |post_image| %>
    <div class ="mb-2 card shadow-lg">
      <div class="card-header">
        <%= link_to post_image.title, post_image_path(post_image), class: "text-dark" %>
      </div>
    <%= image_tag post_image.image, size: "200x150" %>
    </div>
  <% end %>
</div>